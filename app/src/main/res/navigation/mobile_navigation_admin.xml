<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation_admin"
    app:startDestination="@+id/nav_statisticsDashboard">

    <fragment
        android:id="@+id/nav_delete_customers"
        android:name="com.example.navproject.ui.admin.DeleteCustomers.DeleteCustomersFragment"
        android:label="Delete Customers"
        tools:layout="@layout/fragment_delete_customers" />

    <fragment
        android:id="@+id/nav_statisticsDashboard"
        android:name="com.example.navproject.ui.admin.StatisticsDashBoard.StatisticsDashboardFragment"
        android:label="Statistics Dashboard"
        tools:layout="@layout/fragment_statistics_dashboard" />

    <fragment
        android:id="@+id/nav_add_admin"
        android:name="com.example.navproject.ui.admin.AddAdmin.AddAdminFragment"
        android:label="Add New Admin"
        tools:layout="@layout/fragment_add_admin" />

    <fragment
        android:id="@+id/nav_view_all_reservations"
        android:name="com.example.navproject.ui.admin.ViewAllReservations.ViewAllReservationsFragment"
        android:label="View All Reservations"
        tools:layout="@layout/fragment_view_all_reservations" />

    <fragment
        android:id="@+id/nav_special_offers"
        android:name="com.example.navproject.ui.admin.SpecialOffers.SpecialOffersFragment"
        android:label="Special Offers"
        tools:layout="@layout/fragment_special_offers" />

    <fragment
        android:id="@+id/nav_logout"
        android:name="com.example.navproject.ui.logout.logoutFragment"
        android:label="Logout"
        tools:layout="@layout/fragment_logout" />

</navigation>
